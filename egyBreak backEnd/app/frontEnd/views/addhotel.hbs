{{>header}}
<h4 class="mb-3 text-primary">Add New Hotel</h4>
<form method="post" >
    <div class="mb-3">
        <input name="name" placeholder="name" class="form-control">
    </div>
    <div class="mb-3">
        <input name="city" placeholder="city" class="form-control">
    </div>
    <div class="mb-3">
        <input name="email" placeholder="email" class="form-control">
    </div>
    <div class="mb-3">
    <div class="mb-3">
        <input name="phoneHotel" placeholder="phoneHotel" class="form-control">
    </div>
    <div class="mb-3">
        <input name="price" placeholder="price" class="form-control">
    </div>
    <div class="mb-3">
    <div class="mb-3">
        <input name="image" type="file" placeholder="image" class="form-control">
    </div>
        <button class="btn btn-primary">Add</button>
  
</form>
{{>footer}}